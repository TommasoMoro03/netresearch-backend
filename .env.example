# ==============================================
# NetResearch API - Environment Variables
# ==============================================
# Copy this file to .env and fill in your actual values

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=NetResearch

# Together AI Configuration (Hackathon Credits)
TOGETHER_API_KEY=your-together-ai-api-key-here
TOGETHER_BASE_URL=https://api.together.xyz/v1

# Model Name Examples from Together AI:
# - meta-llama/Llama-3.3-70B-Instruct-Turbo (recommended)
# - meta-llama/Meta-Llama-3.1-70B-Instruct-Turbo
# - mistralai/Mixtral-8x7B-Instruct-v0.1
# - Qwen/Qwen2.5-72B-Instruct-Turbo
MODEL_NAME=meta-llama/Llama-3.3-70B-Instruct-Turbo

# Agent Configuration
AGENT_MAX_ITERATIONS=10
AGENT_TIMEOUT=300

# CORS Configuration
CORS_ORIGINS=["http://localhost:3000"]
